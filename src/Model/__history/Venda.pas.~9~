unit Venda;

interface

uses
  System.SysUtils;

type
  TVenda = class
  private
    FID: Integer;
    FDataVenda: TDate;
    FQuantidadeProduto: Integer;
    FPreco: Double;
    FVendedor: Integer;

  public
    property ID: Integer read FID;
    property dataVenda: TDate read FDataVenda;
    property quantidadeProduto: Integer read FQuantidadeProduto;
    property preco: Double read FPreco;
    property vendedor: Integer read FVendedor;
  end;

implementation

end.
