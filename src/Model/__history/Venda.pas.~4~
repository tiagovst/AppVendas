unit Venda;

interface

uses
  System.SysUtils;

type
  TVenda = class
  private
    id: Integer;
    dataVenda: TDate;
    totalProdutos: Integer;
    quantidadeProduto: Integer;
    totalPreco: Double;
    preco: Double;
    vendedor: Integer;
  public
    property vendaId: Integer read FId write Fid;


  end;

implementation

end.
