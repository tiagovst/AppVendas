unit Venda;

interface

uses
  System.SysUtils;

type
  TVenda = class
  private
    FID: Integer;
    FDataVenda: TDate;
    FQuantidadeProduto: Integer;
    FPreco: Double;
    FVendedor: Integer;

  public
    property ID: Integer read FID write FID;
    property dataVenda: TDate read FDataVenda write FDataVenda;
    property quantidadeProduto: Integer read FQuantidadeProduto write FQuantidadeProduto;
    property preco: Double read FPreco write FPreco;
    property vendedor: Integer read FVendedor write FVendedor;
  end;

implementation

end.
